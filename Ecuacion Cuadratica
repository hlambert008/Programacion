
#include "stdafx.h"
#include "conio.h"
#include "math.h"
#include <iostream>
using namespace std;


int main()
{float a, b, c, X1, X2, D, X;
 cout << "Ingrese el valor de A: ";
 cin  >> a;
 cout << "Ingrese el valor de B: ";
 cin  >> b;
 cout << "Ingrese el valor de C: ";
 cin  >> c;
 if (a==0)
  { 
	X = -c/b;
    cout << X;
  }
 else 
   {
    D = (b*b)-(4*a*c);
	 if(D>=0)
      {
	   X1 = (-b/(2*a)- sqrt(D))/(2*a);
	   X2 = (-b/(2*a)+ sqrt(D))/(2*a);
	   cout << X1 << X2;
      }
	 else 
	  cout << "Solucion Imaginaria"; 
     }
 getch();
}
