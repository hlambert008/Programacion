#include "stdafx.h"
#include "math.h"
#include "conio.h"
#include <iostream>
using namespace std;

int main()
{
	 int A,B,S,R,M,D;
	 cout << "Ingrese el dato A: ";
	 cin  >> A;
	 cout << "Ingrese el dato B: ";
	 cin  >> B;
	 S = A + B;
	 R = A - B;
	 D = A / B;
	 M = A * B;
	 cout << "La suma es:" <<S;
	 cout << "\nLa resta es:" <<R;
	 cout << "\nLa multiplicacion es:" <<M;
	 cout << "\nLa division es:" <<D;
	 getch();
	 return 0;

}
