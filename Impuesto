#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;


int main()

{float S, SB, IMP;

     cout << "Ingrese el sueldo Minimo: ";
     cin  >>SB;
     cout << "Ingrese el sueldo para Calcular su Impuesto: ";
     cin  >>S;

    if(S>2*SB)
      IMP = S*0.13;
     else 
      IMP = 0;
    cout << "El Impuesto a Pagar es: "
    cin  >> IMP;
    getch();
    return 0;
}
